{
  "name": "taimos-cdk-constructs",
  "version": "1.0.0",
  "description": "Higher level constructs for AWS CDK",
  "scripts": {
    "clean": "rimraf coverage dist pack doc 'lib/**/*.js' 'lib/**/*.d.ts' 'tst/**/*.js' 'tst/**/*.d.ts'",
    "test": "nyc -x tst -e .ts --temp-directory 'coverage/nyc-output' -r html -r text-summary -r cobertura _mocha --require ts-node/register 'tst/**/*.spec.ts' --reporter nyan",
    "doc": "typedoc --excludeExternals --mode file --out doc lib/",
    "build": "jsii",
    "watch": "jsii -w",
    "build+test": "npm-run-all clean build test",
    "package": "npm-run-all clean build doc && jsii-pacmak",
    "cdk": "cdk"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "pack",
    "targets": {
      "java": {
        "package": "de.taimos.cdk",
        "maven": {
          "groupId": "de.taimos",
          "artifactId": "cdk"
        }
      },
      "python": {
        "distName": "taimos-cdk",
        "module": "taimos_cdk"
      }
    }
  },
  "keywords": [
    "Cloudformation",
    "CDK"
  ],
  "author": {
    "email": "thorsten.hoeger@taimos.de",
    "name": "Thorsten Hoeger",
    "url": "https://www.taimos.de"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/taimos/cdk-constructs.git"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.6.2",
    "@types/sinon": "^7.0.13",
    "aws-cdk": "^1.0.0",
    "chai": "^4.2.0",
    "jsii": "^0.14.0",
    "jsii-pacmak": "^0.14.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@aws-cdk/alexa-ask": "^1.0.0",
    "@aws-cdk/aws-cloudfront": "^1.0.0",
    "@aws-cdk/aws-cloudformation": "^1.0.0",
    "@aws-cdk/aws-codebuild": "^1.0.0",
    "@aws-cdk/aws-codecommit": "^1.0.0",
    "@aws-cdk/aws-codepipeline": "^1.0.0",
    "@aws-cdk/aws-events-targets": "^1.0.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.0.0",
    "@aws-cdk/aws-ecr": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-logs": "^1.0.0",
    "@aws-cdk/aws-secretsmanager": "^1.0.0",
    "@aws-cdk/aws-sam": "^1.0.0",
    "@aws-cdk/aws-s3-deployment": "^1.0.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-cdk/alexa-ask": "^1.0.0",
    "@aws-cdk/aws-cloudfront": "^1.0.0",
    "@aws-cdk/aws-cloudformation": "^1.0.0",
    "@aws-cdk/aws-codebuild": "^1.0.0",
    "@aws-cdk/aws-codecommit": "^1.0.0",
    "@aws-cdk/aws-codepipeline": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-events-targets": "^1.0.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.0.0",
    "@aws-cdk/aws-ecr": "^1.0.0",
    "@aws-cdk/aws-iam": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/aws-secretsmanager": "^1.0.0",
    "@aws-cdk/aws-sam": "^1.0.0",
    "@aws-cdk/aws-s3": "^1.0.0",
    "@aws-cdk/aws-s3-deployment": "^1.0.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.0.0",
    "@aws-cdk/core": "^1.0.0",
    "@aws-cdk/aws-logs": "^1.0.0"
  }
}
